! base16 chalk color scheme 
#define foreground_color #d0d0d0
#define background_color #151515
#define cursor_color     #d0d0d0
#define black            #151515
#define bright_black     #505050
#define red              #fb9fb1
#define bright_red       #fb9fb1
#define green            #acc267
#define bright_green     #acc267
#define yellow           #ddb26f
#define bright_yellow    #ddb26f
#define blue             #6fc2ef
#define bright_blue      #6fc2ef
#define magenta          #e1a3ee
#define bright_magenta   #e1a3ee
#define cyan             #12cfc0
#define bright_cyan      #12cfc0
#define white            #d0d0d0
#define bright_white     #f5f5f5

! special
*.foreground:   foreground_color
*.background:   background_color
*.cursorColor:  cursor_color

! black
*.color0:       black
*.color8:       bright_black

! red
*.color1:       red
*.color9:       bright_red

! green
*.color2:       green
*.color10:      bright_green

! yellow
*.color3:       yellow
*.color11:      bright_yellow

! blue
*.color4:       blue
*.color12:      bright_blue

! magenta
*.color5:       magenta
*.color13:      bright_magenta

! cyan
*.color6:       cyan
*.color14:      bright_cyan

! white
*.color7:       white
*.color15:      bright_white
