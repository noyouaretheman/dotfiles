"*":
  "atom-material-ui":
    colors:
      abaseColor: "#fb9fb1"
      accentColor: "#6fc2ef"
      predefinedColor: "Purple"
    fonts: {}
    tabs:
      compactTabs: true
    treeView:
      compactList: true
    ui:
      animations: false
  "autocomplete-clang":
    includeSystemHeadersDocumentation: true
  "base16-syntax":
    scheme: "Chalk"
  core:
    disabledPackages: [
      "linter-ui-default"
      "linter"
      "linter-gcc"
      "linter-pylama"
      "build"
      "busy"
    ]
    telemetryConsent: "limited"
    themes: [
      "atom-material-ui"
      "base16-syntax"
    ]
  "dark-flat-ui": {}
  editor:
    fontFamily: "DejaVu Sans"
    fontSize: 13
    showIndentGuide: true
    softWrap: true
    softWrapHangingIndent: 4
    tabLength: 4
  "exception-reporting":
    userId: "43fd62fa-3448-44e5-8f9d-75af168f970e"
  "file-watcher":
    promptWhenChange: true
  fonts:
    fontFamily: "DejaVu Mono"
  latex:
    buildOnSave: true
  linter:
    disabledProviders: [
      "Pylama"
    ]
  "linter-ui-default": {}
  minimap:
    autoToggle: false
    plugins:
      codeglance: false
      codeglanceDecorationsZIndex: 0
      cursorline: true
      cursorlineDecorationsZIndex: 0
      "find-and-replace": true
      "find-and-replaceDecorationsZIndex": 0
      "minimap-autohide": false
      "minimap-autohideDecorationsZIndex": 0
      "split-diff": true
      "split-diffDecorationsZIndex": 0
  "package-sync":
    createOnChange: true
    forceOverwrite: true
  "spell-check":
    grammars: [
      "source.asciidoc"
      "source.gfm"
      "text.git-commit"
      "text.plain"
      "text.plain.null-grammar"
      "text.tex.latex"
    ]
  "split-diff":
    hideTreeView: true
    ignoreWhitespace: true
  "tree-view": {}
  "vim-mode-plus":
    clearHighlightSearchOnResetNormalMode: true
    flashOnOperate: false
    flashOnSearch: false
    flashOnUndoRedo: false
    flashScreenOnSearchHasNoMatch: false
    highlightSearch: true
    notifiedCoffeeScriptNoLongerSupportedToExtendVMP: true
    useClipboardAsDefaultRegister: true
    useSmartcaseForSearch: true
    useSmartcaseForSearchCurrentWord: true
  welcome:
    showOnStartup: false
".c.source":
  editor:
    preferredLineLength: 78
    tabLength: 4
".console.python.text":
  editor:
    autoIndentOnPaste: true
    preferredLineLength: 78
    softWrapHangingIndent: 4
    tabLength: 4
".cpp.source":
  editor:
    preferredLineLength: 78
    tabLength: 4
".diff":
  editor:
    preferredLineLength: 78
    tabLength: 4
".python.regexp.source":
  editor:
    preferredLineLength: 78
    softWrapHangingIndent: 4
".python.source":
  editor:
    autoIndentOnPaste: true
    preferredLineLength: 78
    softWrapHangingIndent: 4
".python.text.traceback":
  editor:
    preferredLineLength: 78
    softWrapHangingIndent: 4
    tabLength: 4
